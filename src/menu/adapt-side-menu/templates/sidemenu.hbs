
<div class="menu-container side-menu" role="region" aria-label="{{_globals._menu._boxmenu.ariaRegion}}">
 <div class="menu__header boxmenu__header animated fadeIn delay-05s">
    <div class="menu__header-inner boxmenu__header-inner">

      {{#if displayTitle}}
      <div class="menu__title boxmenu__title">

        {{#unless _disableAccessibleState}}
        <div class="js-heading"></div>
        {{/unless}}

        <div class="menu__title-inner boxmenu__title-inner" {{#unless _disableAccessibleState}}aria-hidden="true"{{/unless}}>
          {{{compile displayTitle}}}
        </div>

      </div>
      {{/if}}

      {{#if subtitle}}
      <div class="menu__subtitle boxmenu__subtitle">
        <div class="menu__subtitle-inner boxmenu__subtitle-inner">
          {{{compile subtitle}}}
        </div>
      </div>
      {{/if}}

      {{#if body}}
      <div class="menu__body boxmenu__body">
        <div class="menu__body-inner boxmenu__body-inner">
          {{{compile body}}}
        </div>
      </div>
      {{/if}}

    </div>
  </div>
 {{#if instruction}}
      <div class="menu__instruction boxmenu__instruction">
        <div class="menu__instruction-inner boxmenu__instruction-inner">
          {{{compile instruction}}}
        </div>
      </div>
      {{/if}}

   <!-- <div class="arrow-image"><img src='assets/arrow.png'></div> -->
	<div class="menu-container-inner box-menu-inner clearfix js-children" role="list" >
	</div>
	<div class="aria-label relative a11y-ignore-focus prevent-default" tabindex="0" role="region">{{_globals._menu._boxmenu.menuEnd}}</div>
</div>
